package test;

public class LoopingExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//print 1 to 10 using while loop
		int num =10;
		System.out.println(++num);
		System.out.println(++num++);
		while (num >=1) {
		System.out.println(num);
		num--;
		}
		System.out.println("end of while loop");
		
	}

}
