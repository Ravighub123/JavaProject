public class LoopingExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//print 1 to 10 using while loop
		int num1 =1;
		int num2=1;
		int i= num1++;
		int j= ++num2;
		System.out.println(i);
		System.out.println(j);
		
